{
  "name": "metronome",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "npm-run-all --parallel watch:assets server",
    "start:prod": "npm-run-all --parallel build:prod server",
    "server": "http-server ./public -c-1",
    "build:prod": "NODE_ENV=production npm run build:assets",
    "build:assets": "npm-run-all --parallel build:css build:js",
    "watch:assets": "npm-run-all --parallel watch:css watch:js",
    "build:js": "webpack",
    "build:css": "postcss src/style.css --config postcss.config.js -d public/dist --ext css",
    "watch:js": "npm run build:js -- --watch",
    "watch:css": "npm run build:css && npm run build:css -- --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "animejs": "^3.1.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.4",
    "cssnano": "^4.1.10",
    "http-server": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "regenerator-runtime": "^0.13.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
